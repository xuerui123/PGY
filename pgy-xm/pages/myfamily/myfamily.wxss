page{
  background: #efefef;
}
.list{
  display: flex;
  width: 100%;
  flex-direction: column; 
}
.list .item{
  display: flex;
  width: 100%;  
  background: #fff;
  align-items: center;
  padding: 30rpx 0;
  border-bottom: 1px solid #eee;
  
}
.list .item .photo{
  display: flex;
  width: 100rpx;
  height:100rpx;
  background: #eee;
  border-radius: 50%;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.list .item .text{
  display: flex;
  flex-direction: column;
  color: #9b9fac;
  width: 70%;
}

